{
  "name": "nuxt-typescript-starter",
  "version": "1.0.0",
  "description": "Nuxt.js + Koa + Typescript + Vuetify starter",
  "author": "Samuel Grenier <samrg472@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -e ts --watch server --exec 'ts-node --files -P tsconfig.server.json server/index.ts'",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production ts-node --files -P tsconfig.server.json server/index.ts",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "koa": "^2.5.2",
    "nuxt": "^2.0.0",
    "nuxt-property-decorator": "^1.3.1",
    "ts-node": "^7.0.1",
    "vuetify": "^1.2.4"
  },
  "devDependencies": {
    "@types/koa": "^2.0.47",
    "@types/node": "^10.12.18",
    "nodemon": "^1.11.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-loader": "^5.3.2",
    "typescript": "^3.2.2"
  }
}
